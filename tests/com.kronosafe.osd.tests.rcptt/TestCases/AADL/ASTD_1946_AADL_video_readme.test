get-view "Model Explorer" | get-tree | get-menu -path "New/Example..." | click
with [get-window "New Example"] {
    get-tree | select "Open System Description Examples/AADL2OSD Transformation Example" | double-click
    get-list | select "com.kronosafe.osd.examples.aadl2osd" | click -default
    get-button Finish | click
}
with [get-view "Model Explorer" | get-tree] {
    select "com.kronosafe.osd.examples.aadl2osd/readme.md" | select "com.kronosafe.osd.examples.aadl2osd/readme.md" 
        | get-menu -path "Open With/Text Editor" | click
}
with [get-editor "readme.md" | get-text-viewer] {
    set-caret-pos 1 2
    select-range 1 1 4 1
}
get-view "Model Explorer" | get-tree 
    | select "com.kronosafe.osd.examples.aadl2osd/video/aadl2osd2psy_producer_consumer_example.html" | double-click
